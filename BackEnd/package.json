{
  "name": "sfa-backend",
  "version": "0.0.1",
  "description": "SFAアプリケーションのバックエンド",
  "main": "handler.js",
  "private": true,
  "scripts": {
    "test": "npm run karma start",
    "svsinit": "serverless config credentials --provider aws -k 'アクセスID' -s 'シークレットキー' -o",
    "deploy": "serverless deploy -v",
    "deployf": "serverless deploy -v --function",
    "remove": "serverless remove -v",
    "info": "serverless info -v",
    "alexaauth": "sls alexa auth",
    "alexainit": "sls alexa create --name SFA-Alexa --locale ja-JP --type custom",
    "alexaupdate": "serverless alexa update",
    "alexabuild": "serverless alexa build",
    "svs": "serverless",
    "karma": "karma"
  },
  "dependencies": {
    "api-interface": "../common",
    "ask-sdk-core": "^2.7.0",
    "ask-sdk-model": "^1.23.1",
    "aws-sdk": "^2.568.0",
    "axios": "^0.21.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "source-map-support": "^0.5.10",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jasmine": "^3.4.6",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.5",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^4.0.2",
    "serverless": "^1.48.4",
    "serverless-alexa-skills": "^0.1.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.4.5",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  },
